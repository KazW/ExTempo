{
  "root": "dist",
  "routes": {
    "/assets/**": "/assets/"
  },
  "headers": {
    "/": {
      "Cache-Control": "no-store, no-cache"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/assets/fonts/*": {
      "Access-Control-Allow-Origin": "*"
    }
  }
}
